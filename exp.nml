				min_value: 0;
				max_value: 127;
				def_value: 30;
				names: {
					0: string(STR_NO_LIMITS);
				};
			}
		}
		param 8 {
			param_dirt {
				type:		int;
				name:		string(STR_PARAM_8_NAME);
				desc:		string(STR_PARAM_8_DESC);
				min_value: 0;
				max_value: 127;
				def_value: 40;
				names: {
					0: string(STR_NO_LIMITS);
				};
			}
		}
		param 9 {
			param_town {
